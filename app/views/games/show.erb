<% content_for :head do %>
  <meta name="game-uid" content="<%= @game_uid %>">

  <% if @participant %>
    <meta name="game-activity-channel" content="GameChannel">
    <meta name="vue-data" content="<%= @view_data.to_json %>">
  <% end %>
<% end %>

<pre style='direction: ltr;'>
<%= JSON.pretty_generate(@view_data) %>
</pre>
